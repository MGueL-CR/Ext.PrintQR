{
    "name": "PrintQR",
    "description": "This is a little extension to read the VPO number and show as a QR code in the page web. Developed by Miguel Mej√≠as A.",
    "action": {
        "default_popup": "ShowPopup.html",
        "default_icon": {
            "16": "/imgs/escan-qr-16.png",
            "32": "/imgs/escan-qr-32.png",
            "64": "/imgs/escan-qr-64.png",
            "128": "/imgs/escan-qr-128.png"
        }},
    "icons": {
        "16": "/imgs/escan-qr-16.png",
        "32": "/imgs/escan-qr-32.png",
        "48": "/imgs/escan-qr-64.png",
        "128": "/imgs/escan-qr-128.png"
    },
    "version": "2.0",
    "manifest_version": 3,
    "content_scripts": [
        {
          "js": ["/JS/qrcode.min.js", "/JS/app.js"],
          "css": ["CSS/styleQR.css"],
          "matches": [
            "file:///C:/Users/*",
            "file:///home/*"
          ]
        }
      ]
}